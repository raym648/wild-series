<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="build/css/app.css">
        {% endblock %}
    </head>


    <body>

        {% block header %}
            {{ include('navbar.html.twig') }}
        {% endblock %}

        <main class="container main">
            {% block content %}{% endblock %}
        </main>

        {% block footer %}{% endblock %}

        {% block javascripts %}
            <script src="build/js/app.js"></script>
        {% endblock %}

    </body>
</html>
